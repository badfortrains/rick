<mujoco model="simple_biped">
    <compiler angle="radian" autolimits="true"/>
    <visual>
        <map force="0.1" zfar="30"/>
        <rgba haze="0.15 0.25 0.35 1"/>
        <global offwidth="2560" offheight="1440" elevation="-20" azimuth="120"/>
    </visual>

    <default>
        <geom type="mesh" xyaxes="1 0 0 0 1 0" rgba="0.5 0.7 1.0 1"/>
        <position kp="1" />
    </default>

    <asset>
        <mesh name="body" file="mesh/body.stl" scale="0.001 0.001 0.001"/>
        <mesh name="leftUpperLeg" file="mesh/leftUpperLeg.stl" scale="0.001 0.001 0.001"/>
        <mesh name="leftLowerLeg" file="mesh/leftLowerLeg.stl" scale="0.001 0.001 0.001"/>
        <mesh name="leftFoot" file="mesh/leftFoot.stl" scale="0.001 0.001 0.001"/>
        <mesh name="rightUpperLeg" file="mesh/rightUpperLeg.stl" scale="0.001 0.001 0.001"/>
        <mesh name="rightLowerLeg" file="mesh/rightLowerLeg.stl" scale="0.001 0.001 0.001"/>
        <mesh name="rightFoot" file="mesh/rightFoot.stl" scale="0.001 0.001 0.001"/>
    </asset>

    <worldbody>
        <geom name="floor" size="0 0 .05" type="plane" condim="3" rgba="0.9 0.9 0.9 1"/>
        <light name="spotlight" mode="targetbodycom" target="body" diffuse=".8 .8 .8" specular="0.3 0.3 0.3" pos="0 -6 4" cutoff="30"/>
        <camera name="track" mode="track" pos="0 -3 1" xyaxes="1 0 0 0 0.2 1" target="body"/>
        <body name="body" pos="0 0 0">
            <freejoint name="root"/>
            <geom mesh="body" density="100"/>
            <body name="leftUpperLeg">
                <joint name="leftHipJoint" axis="0 1 0" pos=".02540 0 .390984" range="-0.1 1"/>
                <joint name="leftUpperLegJoint" axis="1 0 0" pos=".02540 0 .390984" range="-1.2 1.2"/>
                <geom mesh="leftUpperLeg"/>
                <body name="leftLowerLeg">
                    <joint name="leftLowerLegJoint" axis="1 0 0" pos=".02540 0 .187784" range="-1.5 2"/>
                    <geom mesh="leftLowerLeg"/>
                    <body name="leftFoot">
                        <joint name="leftFootJoint" axis="1 0 0" pos=".02540 0 0.02540" range="-2 2"/>
                        <geom mesh="leftFoot"/>
                        <site name="left_foot_site" pos="0.01270 0 0" size="0.01"/>
                    </body>
                </body>
            </body>
            <body name="rightUpperLeg">
                <joint name="rightHipJoint" axis="0 1 0" pos="0.20320 0 .390984" range="-0.1 1"/>
                <joint name="rightUpperLegJoint" axis="1 0 0" pos="0.20320 0 .390984" range="-1.2 1.2"/>
                <geom mesh="rightUpperLeg"/>
                <body name="rightLowerLeg">
                    <joint name="rightLowerLegJoint" axis="1 0 0" pos="0.20320 0 0.187784" range="-1.5 2"/>
                    <geom mesh="rightLowerLeg"/>
                    <body name="rightFoot">
                        <joint name="rightFootJoint" axis="1 0 0" pos="0.20320 0 0.02540" range="-2 2"/>
                        <geom mesh="rightFoot"/>
                        <site name="right_foot_site" pos="0.21590 0 0" size="0.01"/>
                    </body>
                </body>
            </body>
        </body>
    </worldbody>

    <actuator>
        <position name="leftHipJoint" joint="leftHipJoint"  />
        <position name="leftUpperLegJoint" joint="leftUpperLegJoint"  />
        <position name="leftLowerLegJoint" joint="leftLowerLegJoint"  />
        <position name="leftFootJoint" joint="leftFootJoint"  />

        <position name="rightHipJoint" joint="rightHipJoint" />
        <position name="rightUpperLegJoint" joint="rightUpperLegJoint"  />
        <position name="rightLowerLegJoint" joint="rightLowerLegJoint"  />
        <position name="rightFootJoint" joint="rightFootJoint" />
    </actuator>

    <keyframe>
<key
 name="home"
  qpos="3.59599e-06 0.000153245 -0.000507215 1 0.000755206 -2.1714e-07 9.52667e-07 5.58361e-05 3.73092e-05 -0.00075412 -0.000868134 -3.75697e-05 3.81408e-05 -0.000758133 -0.000865443"
  ctrl="0 0 0 0 0 0 0 0"
/>

