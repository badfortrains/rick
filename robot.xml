<mujoco model="simple_biped">
    <compiler angle="radian" autolimits="true" />
    <visual>
        <map force="0.1" zfar="30"/>
        <rgba haze="0.15 0.25 0.35 1"/>
        <global offwidth="2560" offheight="1440" elevation="-20" azimuth="120"/>
    </visual>

    <default>
        <geom type="mesh" xyaxes="1 0 0 0 1 0" rgba="0.5 0.7 1.0 1"/>
        <joint damping="1" />
        <position kp="13" />
    </default>

    <asset>
        <mesh name="body" file="mesh/body.stl" scale="0.001 0.001 0.001"/>
        <mesh name="leftUpperLeg" file="mesh/leftUpperLeg.stl" scale="0.001 0.001 0.001"/>
        <mesh name="leftLowerLeg" file="mesh/leftLowerLeg.stl" scale="0.001 0.001 0.001"/>
        <mesh name="leftFoot" file="mesh/leftFoot.stl" scale="0.001 0.001 0.001"/>
        <mesh name="rightUpperLeg" file="mesh/rightUpperLeg.stl" scale="0.001 0.001 0.001"/>
        <mesh name="rightLowerLeg" file="mesh/rightLowerLeg.stl" scale="0.001 0.001 0.001"/>
        <mesh name="rightFoot" file="mesh/rightFoot.stl" scale="0.001 0.001 0.001"/>
        <mesh name="pelvis" file="mesh/pelvis.stl" scale="0.001 0.001 0.001"/>
    </asset>

    <worldbody>
        <geom name="floor" size="0 0 .05" type="plane" condim="3" rgba="0.9 0.9 0.9 1"/>
        <light name="spotlight" mode="targetbodycom" target="body" diffuse=".8 .8 .8" specular="0.3 0.3 0.3" pos="0 -6 4" cutoff="30"/>
        <body name="body" pos="0 0 0.632591">
            <freejoint  name="root"/>
            <geom mesh="body" density="100"/>
            <body name="pelvis">
                <geom mesh="pelvis" />
                <joint name="pelvisJoint" axis="0 0 1" pos="0 0 -0.17500" range="-2 2"/>
                <body name="leftUpperLeg">
                    <joint name="leftHipJoint" axis="0 1 0" pos="-0.05160 0.01774 -0.242016" range="-0.2 0.6"/>
                    <joint name="leftUpperLegJoint" axis="1 0 0" pos="-0.05160 0.01774 -0.242016" range="-1.5 1.5"/>
                    <geom mesh="leftUpperLeg" density="1500"/>
                    <body name="leftLowerLeg">
                        <joint name="leftLowerLegJoint" axis="1 0 0" pos="-0.05160 0.01774 -0.45520" range="0 2.7"/>
                        <geom mesh="leftLowerLeg" density="1500"/>
                        <body name="leftFoot" >
                            <joint name="leftFootJoint"  axis="1 0 0" pos="-0.05160 0.01774 -0.60760" range="-1 1"/>
                            <joint name="leftAnkleRollJoint" stiffness="5" damping="2" axis="0 1 0" pos="-0.05160 0.01774 -0.60760" range="-0.5 0.5"/>
                            <geom mesh="leftFoot" density="1000" friction="1 0.005 0.0001"/>
                            <site name="left_foot_site" pos="-0.06410 0 -0.63300" size="0.01"/>
                        </body>
                    </body>
                </body>
                <body name="rightUpperLeg">
                    <joint name="rightHipJoint" axis="0 1 0" pos="0.0516 0.01774 -0.242016" range="-0.2 0.6"/>
                    <joint name="rightUpperLegJoint" axis="1 0 0" pos="0.0516 0.01774 -0.242016" range="-1.5 1.5"/>
                    <geom mesh="rightUpperLeg" density="1500"/>
                    <body name="rightLowerLeg">
                        <joint name="rightLowerLegJoint" axis="1 0 0" pos="0.0516 0.01774 -0.45520" range="0 2.7"/>
                        <geom mesh="rightLowerLeg" density="1500"/>
                        <body name="rightFoot">
                            <joint name="rightFootJoint"  axis="1 0 0" pos="0.0516 0.01774 -0.60760" range="-1 1"/>
                            <joint name="rightAnkleRollJoint" stiffness="5" damping="2" axis="0 1 0" pos="0.0516 0.01774 -0.60760" range="-0.5 0.5"/>
                            <geom mesh="rightFoot" density="1000" friction="1 0.005 0.0001"  />
                            <site name="right_foot_site" pos="0.06410 0 -0.63300" size="0.01"/>
                        </body>
                    </body>
                </body>
            </body>
        </body>

        <camera name="track" mode="trackcom" pos="0 -3 1" xyaxes="1 0 0 0 0.2 1" target="body"/>

    </worldbody>

    <actuator>
        <position name="leftHipJoint" joint="leftHipJoint"  />
        <position name="leftUpperLegJoint" joint="leftUpperLegJoint"  />
        <position name="leftLowerLegJoint" joint="leftLowerLegJoint"  />
        <position name="leftFootJoint" joint="leftFootJoint" />


        <position name="rightHipJoint" joint="rightHipJoint" />
        <position name="rightUpperLegJoint" joint="rightUpperLegJoint"  />
        <position name="rightLowerLegJoint" joint="rightLowerLegJoint"  />
        <position name="rightFootJoint" joint="rightFootJoint" />
    </actuator>

    <keyframe>
    </keyframe>
</mujoco>



